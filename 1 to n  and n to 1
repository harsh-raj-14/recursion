//1 to n
class Solution {
  public:
    void printTillN(int n) {
        // code here
        if(n==0) return ;
        printTillN(n-1);
        cout<<n<<" ";
    }
};
//n to 1
class Solution {
  public:
    void printTillN(int n) {
        // code here
        if(n==0) return ;
        cout<<n<<" ";
printTillN(n-1);
    }
};
